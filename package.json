{
  "name": "joola.io.logger",
  "preferGlobal": false,
  "version": "3.0.1",
  "author": "Joola <info@joo.la>",
  "description": "joola.io's logger node",
  "engine": "node >= 0.10.x",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/joola/joola.io.logger.git"
  },
  "contributors": [
    {
      "name": "Itay Weinberger",
      "email": "itay@joo.la",
      "url": "http://joo.la"
    },
    {
      "name": "Or Weinberger",
      "email": "or@joo.la",
      "url": "http://joo.la"
    }
  ],
  "scripts": {
    "test": "make test",
    "debug": "npm-inspector && npm start && open \"http://http://127.0.0.1:8080/debug?port=5858\"",
    "monitor": "mocha --reporter min --watch",
    "debugtest": "mocha --debug-brk --reporter spec",
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules"
    },
    "coveralls": "mocha --require blanket --reporter mocha-lcov-reporter | node ./node_modules/coveralls/bin/coveralls.js",
    "citest": "mocha --reporter xunit > test-reports.xml"
  },
  "main": "./lib/joola.io.logger",
  "dependencies": {
    "winston": "*"
  },
  "devDependencies": {
    "closurecompiler": "*",
    "underscore": "1.x",
    "mocha-lcov-reporter": "0.x",
    "coveralls": "2.x",
    "should": "1.x",
    "mocha": "1.x",
    "sinon": "1.x",
    "chai": "1.x",
    "sinon-chai": "2.x",
    "blanket": "1.x",
    "jshint": "2.x",
    "supertest":"0.x"
  },
  "license": "GPL-3.0"
}
